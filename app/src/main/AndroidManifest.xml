<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fenella.ecocam">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/eco_dark"
        android:label="@string/app_name"
        android:roundIcon="@drawable/eco_dark"
        android:supportsRtl="true"
        android:theme="@style/Theme.EcoCam">
        <activity android:name=".proveresult"></activity>
        <activity android:name=".finalpage" />
        <activity android:name=".fillform" />
        <activity android:name=".CameraPage" />
        <activity android:name=".ScannerActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>